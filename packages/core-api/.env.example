# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/staples-checker

# Server Configuration
PORT=3000
NODE_ENV=development

# Authentication (Optional - leave empty to disable auth completely)
# JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=7d

# Discord OAuth (Optional - leave empty to disable Discord auth)
# DISCORD_CLIENT_ID=your_discord_client_id
# DISCORD_CLIENT_SECRET=your_discord_client_secret
# DISCORD_REDIRECT_URI=http://localhost:3000/auth/oauth/discord/callback

# Google OAuth (Optional - leave empty to disable Google auth)
# GOOGLE_CLIENT_ID=your_google_client_id
# GOOGLE_CLIENT_SECRET=your_google_client_secret
# GOOGLE_REDIRECT_URI=http://localhost:3000/auth/oauth/google/callback